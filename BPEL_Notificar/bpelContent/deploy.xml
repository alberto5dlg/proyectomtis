<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:Aerolinea-1="http://www.example.org/Aerolinea-1/" xmlns:Envios="http://www.example.org/Envios/" xmlns:listaEspera="http://www.example.org/listaEspera/" xmlns:proyecto.mtis="http://proyecto.mtis">
  <process name="proyecto.mtis:BPEL_Notificar">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="proyecto.mtis:BPEL_NotificarService" port="BPEL_NotificarPort"/>
    </provide>
    <invoke partnerLink="ListaEsperaPL">
      <service name="listaEspera:listaEspera" port="listaEsperaSOAP"/>
    </invoke>
    <invoke partnerLink="Aerolinea1PL">
      <service name="Aerolinea-1:Aerolinea-1" port="Aerolinea-1SOAP"/>
    </invoke>
    <invoke partnerLink="CorreosPL">
      <service name="Envios:Envios" port="EnviosSOAP"/>
    </invoke>
  </process>
</deploy>