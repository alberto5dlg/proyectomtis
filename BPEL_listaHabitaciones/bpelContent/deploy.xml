<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:hotelera="http://www.example.org/hotelera/" xmlns:hotelera2="http://www.example.org/hotelera2/" xmlns:listaHabitaciones="http://listaHabitaciones">
  <process name="listaHabitaciones:listaHabitaciones">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="listaHabitaciones:listaHabitacionesService" port="listaHabitacionesPort"/>
    </provide>
    <invoke partnerLink="consultarHotelera1PL">
      <service name="hotelera:hotelera" port="hoteleraSOAP"/>
    </invoke>
    <invoke partnerLink="consultarHotelera2PL">
      <service name="hotelera2:hotelera2" port="hotelera2SOAP"/>
    </invoke>
  </process>
</deploy>