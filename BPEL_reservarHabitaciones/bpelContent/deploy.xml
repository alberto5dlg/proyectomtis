<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:hotelera="http://www.example.org/hotelera/" xmlns:hotelera2="http://www.example.org/hotelera2/" xmlns:reservar="http://reservar">
  <process name="reservar:reservar">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="reservar:reservarService" port="reservarPort"/>
    </provide>
    <invoke partnerLink="reservarHotel1PL">
      <service name="hotelera:hotelera" port="hoteleraSOAP"/>
    </invoke>
    <invoke partnerLink="reservarHotel2PL">
      <service name="hotelera2:hotelera2" port="hotelera2SOAP"/>
    </invoke>
  </process>
</deploy>